<div class='nested-fields form-inline' >
    <div class="form-group mr-2">
      <%= f.select :grape_id, Grape.all.collect { |grape| [grape.name, grape.id] }, prompt: "Choose Grape: " %>
      <% byebug %>
    </div>

    <div class="form-group">
      <%= f.label :percent %> 
      <%= f.number_field :percent %>
    </div>
    
    <%= link_to_remove_association "Remove Strain", f %>
  
</div>